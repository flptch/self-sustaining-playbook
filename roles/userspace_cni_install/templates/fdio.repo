[fdio_{{ vpp_version }}]
name=fdio_{{ vpp_version }}
baseurl=https://packagecloud.io/fdio/{{ vpp_version }}/el/7/$basearch
repo_gpgcheck=1
gpgcheck=0
enabled=1
gpgkey=https://packagecloud.io/fdio/{{ vpp_version }}/gpgkey
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300

[fdio_{{ vpp_version }}-source]
name=fdio_{{ vpp_version }}-source
baseurl=https://packagecloud.io/fdio/{{ vpp_version }}/el/7/SRPMS
repo_gpgcheck=1
gpgcheck=0
enabled=1
gpgkey=https://packagecloud.io/fdio/{{ vpp_version }}/gpgkey
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300

